        {
          switch (param_count)
          {
            case 0:
            {/*{{{*/
              m_meth_descr_lst.push("Creates default object of class `%s`." % class_name);

              print_code = get_code(a_class,"obj","check_print");

              m_meth_ex_code = ("
obj = new %s();
%s
" % [class_name,print_code]);
