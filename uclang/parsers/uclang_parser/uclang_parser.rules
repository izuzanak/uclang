rules:
   <start> -> <end_check> ->> {}

   <end_check> -> <program> _END_ ->> {}
   <end_check> -> _END_ ->> {}

   <program> -> <import_list> ->>
   {
      if gen_parse_tree == 1 then
         this_idx = node_idx;
         node_idx = node_idx + 1;
         print("   node_"..this_idx.." [label = \"<import_list>\"]");
         print("   node_"..this_idx.." -> node_"..table.remove(node_stack).."");
         table.insert(node_stack,this_idx);

         assert(#node_stack == 1);
         print("\}");
      end
   }

