rules:
   <start> -> <end_check> ->> {null}

   <end_check> -> <program> _END_ ->> {null}
   <end_check> -> _END_ ->> {null}

   <program> -> <import_list> ->> 
   {
      if gen_parse_tree
	 this_idx = node_idx++;
	 out("   node_",this_idx," [label = \"<import_list>\"]\n");
	 out("   node_",this_idx," -> node_",pop(node_stack),"\n");
	 push(node_stack,this_idx);

	 assert(size(node_stack) == 1);
	 out("\}\n");
      else
	 null
      fi
   }

